{
  "name": "productsway-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tinacms build && next build",
    "dev": "tinacms dev -c \"next\"",
    "develop": "yarn dev",
    "lint": "next lint",
    "prepare": "husky install",
    "start": "tinacms build && next start"
  },
  "nano-staged": {
    "package.json": "sort-package-json",
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,css,md}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@giscus/react": "2.4.0",
    "@happykit/flags": "3.3.0",
    "@tailwindcss/typography": "0.5.12",
    "@tinacms/cli": "1.5.42",
    "daisyui": "4.9.0",
    "glob": "10.3.10",
    "gray-matter": "4.0.3",
    "just-unique": "4.2.0",
    "kutty": "0.6.0",
    "lite-youtube-embed": "0.3.2",
    "next": "14.1.4",
    "next-seo": "6.5.0",
    "node-glob": "1.2.0",
    "prismjs": "1.29.0",
    "raw-loader": "4.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "9.0.1",
    "remark-gfm": "4.0.0",
    "sharp": "0.33.3",
    "tinacms": "1.6.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "5.0.0",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/node": "20.11.30",
    "@types/react": "18.2.73",
    "autoprefixer": "10.4.19",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.4",
    "husky": "8.0.3",
    "nano-staged": "0.8.0",
    "postcss": "8.4.38",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-preset-env": "9.5.2",
    "prettier": "3.2.5",
    "sort-package-json": "2.9.0",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.3"
  }
}